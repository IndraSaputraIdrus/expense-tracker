{
  "name": "expense-tracker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "bun --filter='*' dev",
    "dev-client": "cd ./packages/client && bun run dev",
    "dev-server": "cd ./packages/server && bun run dev",
    "build": "cd ./packages/server && bun run build && cd ../../ && cd ./packages/client && bun run build"
  },
  "workspaces": [
    "packages/*"
  ]
}
